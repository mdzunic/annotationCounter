{
	"name": "annotationcounter",
	"displayName": "annotationCounter",
	"description": "Finds, counts and jumps to annotations in code, like TODO...",
	"version": "0.0.2",
	"publisher": "M.Dzunic",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.annotationCounter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.annotationCounter",
				"title": "Annotation Counter"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.35",
		"tslint": "^5.18.0",
		"@types/node": "^12.0.12",
		"@types/mocha": "^5.2.7"
	}
}